{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"type": "project",
    "repositories": [
      {
        "type": "vcs",
        "url": "https://github.com/rattfieldnz/laravel-sitemap",
        "branch": "dev-master"
      }
    ],
	"require": {
		"laravel/framework": "5.1.*",
        "laravelcollective/html": "~5.1",
        "linusu/bitcoin-address-validator": "0.1.2",
		"raven/raven": "0.10.*",
		"guzzlehttp/guzzle": "^6.1",
		"michelf/php-markdown": "~1.5",
		"barryvdh/laravel-ide-helper": "~2.0",
		"doctrine/dbal": "~2.5",
		"way/generators": "~3.0",
		"cviebrock/eloquent-sluggable": ">=3.0.0-beta",
		"rattfieldnz/url-validation": "dev-master",
        "roumen/sitemap": "dev-master",
		"abraham/twitteroauth": "^0.5.3",
		"roumen/feed": "^2.9",
		"chromabits/purifier": "^2.1"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.0",
		"phpspec/phpspec": "~2.1",
		"laracasts/generators": "~1.0",
	    "facebook/php-sdk-v4": "~5.0"
	},
	"autoload": {
		"classmap": [
			"database",
            "app"
		],
		"psr-4": {
			"App\\": "app/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php -r \"copy('.env.example', '.env');\"",
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	}
}
